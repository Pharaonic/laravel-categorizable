{
    "name": "pharaonic/laravel-categorizable",
    "description": "Model Categorizable Management.",
    "keywords": [
        "laravel categorizable",
        "php categorizable",
        "categorizable",
        "laravel",
        "php"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Moamen Eltouny (Raggi)",
            "email": "raggi@raggitech.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "laravel/framework": ">=6.0",
        "pharaonic/laravel-translatable": ">=1.0",
        "pharaonic/laravel-sluggable": ">=1.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pharaonic\\Laravel\\Categorizable\\CategorizableServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Pharaonic\\Laravel\\Categorizable\\": "src"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}